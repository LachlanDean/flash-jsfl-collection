//Find in Libraryvar doc=fl.getDocumentDOM();var query="Symbol";var results=new Array();if(!doc) {	alert("Please open a file before using the Find In Library command");} else {	if(doc.selection[0]!=null && doc.selection[0].elementType=="instance"){		fl.runScript(fl.configURI+"Commands/teknision/utilities/tekLibUtilities.jsfl","setAllFolders",false);		fl.runScript(fl.configURI+"Commands/teknision/utilities/tekLibUtilities.jsfl","expandAndSelect",doc.selection[0].libraryItem.name,false);	}else{				var win=doc.xmlPanel(fl.configURI+"Commands/teknision/windows/find_in_Library.xml");		if(win.dismiss!="cancel"){			var query=win.query;			if(win.gotoedit=="true"){				var editview=true;			}else{				var editview=false;			}									var lib=doc.library;			var items=lib.items;						for(var i=0;i<items.length;i++){				var item=items[i];				var pathsplit=item.name.split("/");				var mainname=pathsplit[pathsplit.length-1];								var inname=mainname.split(query);								if(inname.length>1){					results.push(item);				}																			}			results.reverse();						if(results.length>0){				fl.runScript(fl.configURI+"Commands/teknision/utilities/tekLibUtilities.jsfl","setAllFolders",false);				fl.runScript(fl.configURI+"Commands/teknision/utilities/tekLibUtilities.jsfl","expandAndSelect",results[0].name,editview);			}else{				alert("No Symbol Found");			}		}	}		}